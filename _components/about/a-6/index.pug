section.dh-about-6#dh-about-6(style="background-image: url('./img/about/about_4_bg.jpg')")
	.container
		h2.h-title.text-upper.line Thành tựu nổi bật
		.row
			.col.award
				.swiper-button-prev
					span.mdi.mdi-chevron-left
				.swiper-button-next
					span.mdi.mdi-chevron-right
				.swiper-container
					.swiper-wrapper
						- for ( var x = 1; x < 6; x++)
							.swiper-slide( data-aos="fade-right")
								img(src="./img/about/about_merit_"+x+".jpg", alt="")
